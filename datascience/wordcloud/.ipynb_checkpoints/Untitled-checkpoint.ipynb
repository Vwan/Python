   "source": [
    "from wordcloud import WordCloud\n",
    "\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "filename = \"words.txt\"\n",
    "with open(filename) as f:\n",
    "    mytext = f.read()\n",
    "mytext\n",
    "\n",
    "wordcloud = WordCloud().generate(mytext)\n",
    "\n",
    "%pylab inline\n",
    "plt.imshow(wordcloud, interpolation='bilinear')\n",
    "plt.axis(\"off\")"
   ]
  }
